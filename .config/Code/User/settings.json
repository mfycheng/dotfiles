{
    "editor.lineNumbers": "relative",
    "window.zoomLevel": 0,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "editor.rulers": [
        80
    ],
    "go.inferGopath": false,
    "go.testFlags": [
        "-v"
    ],
    "go.formatFlags": [
        "-local",
        "github.com/kikinteractive"
    ],
    "cSpell.userWords": [
        "Chans",
        "DBAPI",
        "Debugf",
        "GRPC",
        "Infof",
        "Paginator",
        "Tigase",
        "Tracef",
        "Warnf",
        "Wrapf",
        "XMPP",
        "aliaser",
        "aliasjid",
        "awsutil",
        "clientv",
        "cmdutil",
        "commonv",
        "convostatus",
        "coreos",
        "deserialization",
        "deserialize",
        "dynamodb",
        "dynamodbattribute",
        "dynamodbav",
        "dynamodbiface",
        "etcd",
        "etcdserver",
        "etcdtest",
        "fsutil",
        "godoc",
        "groupsv",
        "invalidators",
        "kikdev",
        "kikinteractive",
        "logrus",
        "mvcc",
        "mvccpb",
        "protobuf",
        "ptrs",
        "repo",
        "retriable",
        "serv",
        "stretchr",
        "templateutil",
        "unmarshalling",
        "uuid",
        "xicommon",
        "xiphias",
        "xiphiasapp",
        "xiregistry",
        "xiserver",
        "xiutil",
        "zkconfig"
    ],
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "proto",
        "python",
        "pub",
        "restructuredtext",
        "rust",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "explorer.confirmDragAndDrop": false,
    "breadcrumbs.enabled": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "zenMode.hideLineNumbers": false,
    "workbench.colorTheme": "One Dark Pro",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "go.lintTool": "golangci-lint",
}
